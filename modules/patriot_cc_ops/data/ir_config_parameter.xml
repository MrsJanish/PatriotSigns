<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- CC Credentials - User must configure these -->
        <record id="cc_ops_email" model="ir.config_parameter">
            <field name="key">cc_ops.email</field>
            <field name="value"></field>
        </record>
        <record id="cc_ops_password" model="ir.config_parameter">
            <field name="key">cc_ops.password</field>
            <field name="value"></field>
        </record>
        <record id="cc_ops_github_token" model="ir.config_parameter">
            <field name="key">cc_ops.github_token</field>
            <field name="value"></field>
        </record>
    </data>
</odoo>
